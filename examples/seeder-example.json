{
  /* number of seeding peers to start up */
  "NumSeeders" : 5,
  /* root directory for torrents */
  "RootDirectory" : "/mnt/disk/torrents/",
  /* i2p options */
  "I2P" : {
    /* base name of sam session */
    "Session" : "bulkseeder",
    /* change this to where SAM is located if it's at a non standard address */
    "Addr": "127.0.0.1:7656",
    /* sam session options */
    "Opts" : {
      "outbound.quantity" : "6",
      "inbound.quantity" : "2",
      "outbound.length" : "1",
      "inbound.length" : "1"
    }
  }
}
